 <root BTCPP_format="4" >
    <TreeNodeParams>
        <Param name="move_group"/>
        <Param name="joint_values"/>
    </TreeNodeParams>
     <BehaviorTree ID="TemplateMoveToJointSpaceGoal">
        <Sequence name="root">
            <SetMoveItJointValueTarget      joint_values="{joint_values}" move_group="{move_group}"/>
            <PlanMoveItTarget               robot_trajectory_msg="{robot_trajectory_msg}" move_group="{move_group}" />
            <ExecuteMoveItTrajectory        action_name="execute_trajectory" robot_trajectory_msg="{robot_trajectory_msg}"/>
        </Sequence>
     </BehaviorTree>
 </root>
 