 <root BTCPP_format="4" >
    <TreeNodeParams>
        <Param name="move_group"/>
        <Param name="position_xyz"/>
        <Param name="orientation_xyzw"/>
        <Param name="reference_frame"/>
        <Param name="visual_tools"/>
    </TreeNodeParams>
     <BehaviorTree ID="TemplateMoveToPose">
        <Sequence name="root">
            <CreateStampedPose          reference_frame="{reference_frame}" position_xyz="{position_xyz}" orientation_xyzw="{orientation_xyzw}" stamped_pose="{stamped_pose}"/>
            <VisualizePose              stamped_pose="{stamped_pose}" visual_tools="{visual_tools}"/>
            <SetMoveItPoseStampedTarget stamped_pose="{stamped_pose}" move_group="{move_group}"/>
            <PlanMoveItTarget           robot_trajectory_msg="{robot_trajectory_msg}" move_group="{move_group}"/>
            <VisualizeTrajectory        robot_trajectory_msg="{robot_trajectory_msg}" move_group="{move_group}" visual_tools="{visual_tools}"/>
            <ExecuteMoveItTrajectory    action_name="execute_trajectory" robot_trajectory_msg="{robot_trajectory_msg}"/>
            <ClearMarkers               visual_tools="{visual_tools}"/>
        </Sequence>
     </BehaviorTree>
 </root>